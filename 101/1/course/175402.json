[{"name": "\u8449\u60e0\u862d", "email": "hlyeh@ntut.edu.tw", "latestUpdate": "2012-09-16 23:43:13", "objective": "\u672c\u8ab2\u7a0b\u65e8\u5728\u57f9\u990a\u5b78\u751f\u6210\u70ba\u5177\u6709\u6df1\u5ea6\u6587\u5316\u8996\u89ba\u7684\u79d1\u6280\u4eba\u3002\u5f9e\u8a8d\u8b58\u672c\u571f\u6587\u5316\u70ba\u8d77\u9ede\uff0c\u9032\u800c\u8a8d\u8b58\u4e16\u754c\u6587\u5316\u3002\u5f15\u5c0e\u5b78\u751f\u5982\u4f55\u63a2\u7a76\u6587\u5316\u73fe\u8c61\uff0c\u4e26\u4e14\u6df1\u5165\u6587\u5316\u7684\u7cbe\u795e\u5883\u754c\uff0c\u7269\u8cea\u5883\u754c\uff0c\u77ad\u89e3\u6587\u5316\u50f9\u503c\u8207\u6587\u5316\u50b3\u7d71\uff0c\u8b93\u6587\u5316\u89c0\u9ede\u5145\u5206\u878d\u5165\u500b\u4eba\u7684\u601d\u7dad\u5167\u6db5\u3002", "schedule": "\u6587\u5316\u6982\u8ad6\u7cfb\u5217\r\n \u2500 \u5668\u7269\u6587\u5316 \u2500\r\n\r\n1\u3001\u524d\u8ad6\uff1a\u6587\u7269 / \u53e4\u5668\u7269 / \u9748\u7269\u4e03\u7528\u3008\u5206\u7d44\u3009\r\n2\u3001\u4ee5\u7389\u4e8b\u795e \u2500 \u9748\u7269\u6982\u5ff5\u8207\u7389\u5668\u7684\u795e\u5316\r\n3\u3001\u4ee5\u7389\u4e8b\u4eba \u2500 \u7389\u5668\u7684\u6b0a\u5316\u3001\u79ae\u5316\r\n4\u3001\u4ee5\u7389\u986f\u5fb7 \u2500 \u7389\u5668\u7684\u5fb7\u5316\r\n5\u3001\u4ee5\u7389\u70ba\u98fe \u2500 \u7389\u5668\u7684\u98fe\u54c1\u5316\u3008\u4f69\u6234\u9ad4\u9a57 \u2500\u7389\u9805\u98fe\u3009\r\n6\u3001\u4ee5\u7389\u6582\u846c \u2500 \u7389\u5668\u7684\u795e\u79d8\u5316\r\n7\u3001\u4ee5\u7389\u5bd3\u745e \u2500 \u7389\u5668\u7684\u5409\u7965\u5316\u3008\u5e2b\u751f\u4e92\u8d08\u7389\u8a2d\u8a08\u5be6\u52d9\u3009\r\n8\u3001\u4ee5\u7389\u70ba\u5bf6 \u2500 \u7389\u5668\u7684\u50f9\u503c\u5316\u3001\u85cf\u54c1\u5316\r\n9\u3001\u671f\u4e2d\u8003 \r\n10\u3001\u91d1\u5668 \u2500 \u9ec3\u91d1\u6eff\u8db3\u4e16\u9593\u4e00\u5207 \r\n11\u3001\u91d1\u5668 \u2500 \u8ffd\u5c0b\u5f69\u8679\r\n12\u3001\u4e2d\u83ef\u7528\u9999\u6587\u5316\u2500\u9999\u7210\u4e4b\u7528\r\n13\u3001\u4e2d\u83ef\u7528\u9999\u6587\u5316\u2500\u54c1\u9999\u793a\u7bc4\r\n14\u3001\u9285\u5668\u9298\u6587\u7684\u5e7b\u5316 / \u6f06\u5668\u4e4b\u7f8e\r\n15\u3001\u7af9\u6728\u679c\u6838\u8c61\u7259\u7280\u89d2 / \u591a\u5bf6\u683c\u53ca\u53e4\u50a2\u4ff1\r\n16\u3001\u5668\u7269\u9452\u8cde\u8981\u9818\u3008\u7576\u4ee3\u4f69\u98fe\u9ad4\u9a57\u53d6\u6a23 \u2500 \u6a5f\u68b0\u9336\u3009\r\n17\u3001\u6587\u7269\u4fdd\u5b58\u8207\u7ba1\u7406\t\r\n18\u3001 \u671f\u672b\u8003 / \u5340\u57df\u6587\u7269\u5546\u5e97\u5de1\u79ae\u5206\u7d44\u4f5c\u696d\u5c55\u793a", "scorePolicy": "\u671f\u4e2d\u7b46\u8a6650%\r\n\u671f\u672b\u6e2c\u9a5730%\r\n\u6d3b\u52d5\u53c3\u820720%", "materials": "\u3000", "foreignLanguageTextbooks": false}]