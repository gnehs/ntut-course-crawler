[{"name": "\u4efb\u8cbd\u5747", "email": "jyjun@ntut.edu.tw", "latestUpdate": "2012-01-18 17:21:48", "objective": "\u672c\u8ab2\u7a0b\u7684\u76ee\u7684\u662f\u70ba\u4e86\uff1a1.\u9a57\u8b49\u7269\u7406\u7406\u8ad6\u4e4b\u771f\u5be6\u6027\uff0c2.\u57f9\u990a\u540c\u5b78\u8a8d\u771f\u7684\u79d1\u5b78\u614b\u5ea6\u8207\u65b9\u6cd5\uff0c3.\u57f9\u990a\u540c\u5b78\u6578\u64da\u8655\u7406\u7684\u80fd\u529b\uff0c4.\u555f\u767c\u540c\u5b78\u81ea\u884c\u8a2d\u8a08\u89e3\u6c7a\u91cf\u5ea6\u554f\u984c\u7684\u65b9\u6cd5\u3002\u8ab2\u7a0b\u4e4b\u65bd\u884c\u5206\u70ba\u4e0a\u4e0b\u5169\u5b78\u671f\uff0c\u6bcf\u4e00\u5b78\u671f\u70ba\u4e00\u5b78\u5206\u4e09\u5c0f\u6642\u3002\u4e0a\u5b78\u671f\u7684\u5167\u5bb9\u504f\u91cd\u529b\u5b78\u3001\u6ce2\u52d5\u3001\u71b1\u529b\u3001\u6d41\u529b\u4e4b\u5be6\u9a57\uff0c\u4e0b\u5b78\u671f\u7684\u5167\u5bb9\u5247\u504f\u91cd\u96fb\u78c1\u3001\u5149\u5b78\u3001\u8fd1\u7269\u4e4b\u5be6\u9a57\u3002", "schedule": "1.\u6e96\u5099\u54682.\u96fb\u5100\u8868\u4e4b\u4f7f\u7528\u53ca\u6b50\u59c6\u5b9a\u5f8b3.\u514b\u5e0c\u548c\u592b\u5b9a\u5f8b4.\u96fb\u529b\u7dda\u7684\u63cf\u7e6a5.\u96fb\u5bb9\u5668\u4e4b\u5145\u653e\u96fb6.\u96fb\u5b50\u8377\u8cea\u6bd4\u4e4b\u6e2c\u5b9a7.\u87ba\u7dda\u7ba1\u4e2d\u78c1\u5834\u5be6\u9a578.\u5730\u78c1\u6e2c\u91cf9.\u671f\u4e2d\u8003\u546810.\u793a\u6ce2\u5668\u4e4b\u4f7f\u752811.\u4ea4\u6d41\u96fb\u8def12.\u6298\u5c04\u7387\u6e2c\u91cf13.\u7403\u9762\u93e1\u6210\u50cf14.\u8584\u900f\u93e1\u6210\u50cf15.\u7269\u7406\u5149\u5b7816.\u5149\u96fb\u6548\u61c9\u5be6\u9a5717.\u8996\u807d\u6559\u5b7818.\u671f\u672b\u6e2c\u9a57", "scorePolicy": "\u5831\u544a\u6210\u7e3e60%\r\n\u5be6\u9a57\u614b\u5ea630%\r\n\u671f\u672b\u800310%", "materials": "\u666e\u901a\u7269\u7406\u5be6\u9a57\u53ca\u666e\u901a\u7269\u7406\u5be6\u9a57\u5831\u544a  \u53f0\u5317\u79d1\u6280\u5927\u5b78\u5149\u96fb\u7cfb\u6240\u7de8\u8457 \u9ad8\u7acb\u5716\u66f8", "foreignLanguageTextbooks": false}]