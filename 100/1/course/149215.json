[{"name": "\u9ec3\u5fd7\u5f18", "email": "huangch@ntut.edu.tw", "latestUpdate": "2011-09-16 13:30:09", "objective": "\u672c\u8ab2\u7a0b\u4e3b\u8981\u5728\u8b1b\u89e3\u4e00\u822c\u90fd\u5e02\u8a08\u756b\u7684\u4e3b\u8981\u5167\u5bb9\uff0c\u540c\u6642\u91dd\u5c0d\u53f0\u7063\u73fe\u884c\u90fd\u5e02\u8a08\u756b\u7684\u6709\u95dc\u7d44\u7e54\u9ad4\u7cfb\u3001\u5be6\u52d9\u4f5c\u696d\u8207\u73fe\u6cc1\u554f\u984c\u4f5c\u6709\u7cfb\u7d71\u7684\u63a2\u8a0e\u3002", "schedule": "\u6388\u8ab2\u4e3b\u984c\u6db5\u84cb\uff1a1.\u53f0\u7063\u7a7a\u9593\u898f\u5283\u7684\u968e\u5c64\u9ad4\u7cfb\u30012.\u90fd\u5e02\u8a08\u756b\u7d44\u7e54\u9ad4\u7cfb\u8207\u529f\u80fd\u30013.\u53f0\u7063\u90fd\u5e02\u8a08\u756b\u7684\u767c\u5c55\u6cbf\u9769\u30014.\u90fd\u5e02\u898f\u5283\u7684\u7a0b\u5e8f\u3001\u5167\u5bb9\u8207\u65b9\u6cd5\u30015.\u898f\u5283\u8cc7\u6599\u7684\u641c\u96c6\u3001\u8abf\u67e5\u8207\u5206\u6790\u30016.\u571f\u5730\u4f7f\u7528\u8a08\u756b\u30017.\u4ea4\u901a\u8a08\u756b\u30018.\u516c\u5171\u8a2d\u65bd\u8a08\u756b\u30019.\u90fd\u5e02\u66f4\u65b0\u300110.\u90fd\u5e02\u8a08\u756b\u672a\u4f86\u7684\u767c\u5c55\u8da8\u52e2\u300111.\u5be6\u969b\u6848\u4f8b\u5206\u6790\u3001\u7814\u8a0e\u3002", "scorePolicy": "\u3000", "materials": "\u90fd\u5e02\u8a08\u756b\u5b78\u5c0e\u8ad6\uff08\u9322\u5b78\u9676\uff09\u3001\u90fd\u5e02\u8a08\u756b\u6982\u8981\uff08\u4e8e\u660e\u8aa0\uff09\u3001\u90fd\u5e02\u8a08\u756b\u8207\u884c\u653f\uff08\u6797\u82f1\u5f65\u7b49\uff09\u3001\u571f\u5730\u4f7f\u7528\u8a08\u756b\u8207\u8ca1\u7522\u6b0a\uff08\u908a\u6cf0\u660e\uff09", "foreignLanguageTextbooks": false}]