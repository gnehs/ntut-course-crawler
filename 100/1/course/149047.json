[{"name": "\u5289\u5927\u660c", "email": "tcliu@ntut.edu.tw", "latestUpdate": "2011-09-16 15:57:11", "objective": "\u672c\u8ab2\u7a0b\u4e4b\u76ee\u7684\u70ba\u4f7f\u5b78\u751f\u77ad\u89e3\u751f\u7522\u8a08\u756b\u8207\u7ba1\u5236\u7684\u57fa\u672c\u6982\u5ff5\uff0c\u8a13\u7df4\u5b78\u751f\u5c0d\u65bc\u751f\u7522\u8a08\u756b\u8207\u7ba1\u5236\u7684\u5404\u9805\u65b9\u6cd5\uff0c\u5236\u5ea6\u8a2d\u8a08\u53ca\u6539\u5584\u7b49\u5177\u6709\u57fa\u672c\u8a8d\u8b58\uff0c\u4e26\u61c9\u7528\u7ba1\u7406\u79d1\u5b78\u53ca\u4f5c\u696d\u7814\u7a76\u65bc\u751f\u7522\u8a08\u756b\u8207\u7ba1\u5236\u4e4b\u7814\u7a76\u767c\u5c55\u3002\u672c\u8ab2\u7a0b\u5305\u62ec\u4e0b\u5217\u4e3b\u984c\uff1a\u751f\u7522\u8a08\u756b\u8207\u7ba1\u5236\u6982\u8ad6\u3001\u7522\u54c1\u8a2d\u8a08\u8207\u767c\u5c55\u3001\u9810\u6e2c\u3001\u4f86\u6e90\u9700\u6c42\u898f\u756b\u3001\u751f\u7522\u6392\u7a0b\u3001\u7e3d\u6392\u7a0b\u3001\u5b58\u8ca8\u7ba1\u7406\u3001\u8c50\u7530\u5f0f\u7ba1\u7406\u3001\u5c08\u6848\u898f\u756b\u8207\u7ba1\u7406\u7b49\u3002", "schedule": "\u751f\u7522\u8207\u4f5c\u696d\u7ba1\u7406\u6982\u8aaa\n\u7af6\u722d\u529b\u8207\u751f\u7522/\u4f5c\u696d\u7b56\u7565\n\u9810\u6e2c\n\u7522\u80fd\u898f\u5283\n\u88fd\u7a0b\u9078\u64c7\u8207\u8a2d\u65bd\u4f48\u7f6e\n\u5de5\u4f5c\u7cfb\u7d71\u8a2d\u8a08\n\u5730\u9ede\u898f\u5283\n\u5b58\u91cf\u7ba1\u5236\n\u7e3d\u5408\u898f\u5283\n\u7269\u6599\u9700\u6c42\u8a08\u756b\u8207\u7cfb\u7d71\u8cc7\u6e90\u898f\u5283\n\u525b\u597d\u5373\u6642\u751f\u7522\u7cfb\u7d71\n\u65e5\u7a0b\u5b89\u6392\n\u5c08\u6848\u65e5\u7a0b\u7ba1\u7406", "scorePolicy": "\u4f5c\u696d\u8207\u671f\u672b\u5831\u544a:30%\n\u8ab2\u5802\u9910\u8207:10%\n\u671f\u4e2d\u8003:30%\n\u671f\u672b\u8003:30%", "materials": "\u751f\u7522\u7ba1\u7406(\u56db\u7248),\u5f35\u4fdd\u9686,\u9673\u6587\u8ce2,\u8523\u660e\u6643,\u59dc\u9f4a\u7b49\u8457,\u83ef\u6cf0\u6587\u5316\u51fa\u7248", "foreignLanguageTextbooks": false}]