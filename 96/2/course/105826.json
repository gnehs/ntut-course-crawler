[{"name": "\u9673\u4ecb\u5c71", "email": "js.chen@bsmi.gov.tw", "latestUpdate": "2008-03-27 09:30:24", "objective": "\u54c1\u8cea\u7ba1\u5236\u662f\u7528\u4f86\u63a2\u8a0e\u7d93\u71df\u6216\u7ba1\u7406\u5de5\u5ee0\u7684\u65b9\u6cd5\uff0c\u5373\u4ee5\u54c1\u8cea\u7ba1\u5236\u4eba\u54e1\u7684\u7acb\u5834\uff0c\u7814\u7a76\u5982\u4f55\u61c9\u7528\u6700\u6709\u6548\u7684\u65b9\u6cd5\uff0c\u4f86\u7d44\u5408\u61c9\u7528\u5404\u9805\u751f\u7522\u8981\u7d20\uff0c\u4ee5\r\n\u7372\u5f97\u5de5\u5ee0\u7d93\u71df\u7684\u76ee\u6a19\u3002", "schedule": "\u7b2c\u4e00\u9031:      \u54c1\u8cea\u7ba1\u5236\u6982\u8ad6\r\n\u7b2c\u4e8c.\u4e09\u9031:    \u5168\u9762\u54c1\u8cea\u7ba1\u7406\r\n\u7b2c\u56db.\u4e94\u9031:    \u54c1\u8cea\u7cfb\u7d71\r\n\u7b2c\u516d.\u4e03\u9031:    \u54c1\u8cea\u6210\u672c\r\n\u7b2c\u516b\u9031:       \u54c1\u8cea\u7d44\u7e54\r\n\u7b2c\u4e5d.\u5341.\u5341\u4e00\u9031:  \u7d71\u8a08\u6280\u8853\r\n\u7b2c\u5341\u4e8c\u9031:     \u54c1\u8cea\u6aa2\u67e5\r\n\u7b2c\u5341\u4e09.\u5341\u56db\u9031:\u54c1\u8cea\u7ba1\u5236\r\n\u7b2c\u5341\u4e94.\u5341\u516d\u9031:\u54c1\u7ba1\u5708\r\n\u7b2c\u5341\u4e03\u9031:     \u62bd\u6a23\u6aa2\u9a57\r\n\u7b2c\u5341\u516b\u9031:     \u671f\u672b\u8003", "scorePolicy": "\u671f\u672b\u800360%\uff0c\u7fd2\u984c20%\uff0c\u8207\u4e0a\u8ab2\u8868\u73fe20%\u3002", "materials": "\u53c3\u8003\u66f8\u76ee:\u9673\u6c38\u7521\uff0c\u54c1\u8cea\u7ba1\u5236\uff0c\u6587\u4eac\u5716\u66f8\u516c\u53f8\uff0c2005\u5e74\u7248\u3002", "foreignLanguageTextbooks": false}]