[{"name": "\u912d\u9e97\u73b2", "email": "liling@ntut.edu.tw", "latestUpdate": "2007-09-14 12:49:08", "objective": "\u672c\u8ab2\u7a0b\u7684\u8a2d\u8a08\u4ee5\u6b77\u53f2\u8da8\u52e2\u7684\u95e1\u8ff0\u53ca\u5c08\u984c\u7814\u7a76\u5206\u6790\u4e8c\u65b9\u9762\u540c\u6642\u9032\u884c\u3002\u8ab2\u7a0b\u65b9\u9762\u5305\u62ec\u4e2d\u3001\u897f\u6b77\u53f2\u89c0\u5ff5\u7684\u6bd4\u8f03\u8207\u4e2d\u570b\u6b77\u4ee3\u653f\u6cbb\u3001\u7d93\u6fdf\u3001\u793e\u6703\u3001\u6587\u5316\u7684\u6b77\u53f2\u6f14\u9032\uff0c\u85c9\u4ee5\u5e6b\u52a9\u5b78\u751f\u8a8d\u8b58\u6642\u4ee3\u4e4b\u8da8\u52e2\u8207\u610f\u7fa9\u3002\u6388\u8ab2\u65b9\u5f0f\u4ee5\u53e3\u982d\u8b1b\u8ff0\u70ba\u4e3b\uff0c\u8a0e\u8ad6\u53ca\u66f8\u9762\u5831\u544a\u70ba\u8f14\uff0c\u671f\u4f7f\u5b78\u751f\u5728\u77ed\u6642\u9593\u5167\uff0c\u80fd\u5c0d\u4e2d\u570b\u6b77\u53f2\u6709\u4e00\u7c21\u8981\u800c\u6e05\u6670\u7684\u4e86\u89e3\uff0c\u6df1\u5316\u5b78\u751f\u7684\u6b77\u53f2\u611f\u3002", "schedule": "\u4e2d\u570b\u53f2\u8ab2\u7a0b\u5927\u7db1\r\n\u4e00\u3001\u8ab2\u7a0b\u4ecb\u7d39\u8207\u7c4c\u8b00\u65b0\u793e\u6703\r\n\u4e8c\uff5e\u4e09\u3001\u7c4c\u8b00\u65b0\u793e\u6703\r\n\ufffd\u91e7\u7b49\u869e\\\u8b80\uff1a\u52d5\u7269\u8fb2\u838a\r\n\u4e09\uff5e\u56db\u3001\u6df1\u5316\u9769\u547d\r\n\ufffd\u91e7\u7b49\u869e\\\u8b80\uff1a\u5f71\u7247\u8cde\u6790\r\n\u4e94\uff5e\u516d\u3001\u91cd\u555f\u9580\u6236\r\n\u4e03\u3001\u9127\u5c0f\u5e73\u6642\u4ee3\r\n\u516b\u3001\u671f\u4e2d\u8a55\u91cf", "scorePolicy": "\u5e73\u6642\u8a55\u91cf30%\r\n\u671f\u4e2d\u8a55\u91cf30%\r\n\u671f\u672b\u8a55\u91cf40%", "materials": "\u53f2\u666f\u9077\u300a\u8ffd\u5c0b\u73fe\u4ee3\u4e2d\u570b\u300b\u3001\u9673\u6c38\u767c\u300a\u4e2d\u570b\u5171\u7522\u9769\u547d\u4e03\u5341\u5e74\u300b", "foreignLanguageTextbooks": false}]