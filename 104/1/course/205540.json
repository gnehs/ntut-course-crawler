[{"name": "\u9673\u4ecb\u5c71", "email": "js.chen@bsmi.gov.tw", "latestUpdate": "2016-05-25 11:51:04", "objective": "\u5c08\u696d\u6280\u8853\u4e0d\u65b7\u958b\u5275\u8207\u52aa\u529b\uff0c\u5982\u679c\u518d\u914d\u5408\u7ba1\u7406\u7d93\u71df\u7684\u80fd\u529b\uff0c\u5373\u80fd\u76f8\u5f97\u76ca\u5f70\uff0c\u6210\u5c31\u975e\u51e1\u3002\u5de5\u5ee0\u7ba1\u7406\u662f\u7528\u4f86\u63a2\u8a0e\u7d93\u71df\u6216\u7ba1\u7406\u5de5\u5ee0\u7684\u65b9\u6cd5\uff0c\u5373\u4ee5\u5de5\u5ee0\u7ba1\u7406\u4eba\u54e1\u7684\u7acb\u5834\uff0c\u7814\u7a76\u5982\u4f55\u61c9\u7528\u6700\u6709\u6548\u7684\u65b9\u6cd5\uff0c\u4f86\u7d44\u5408\u61c9\u7528\u5404\u9805\u751f\u7522\u8981\u7d20\uff0c\u4ee5\u7372\u5f97\u5de5\u5ee0\u7d93\u71df\u7684\u76ee\u6a19\u3002\u76ee\u524d\u5de5\u696d\u767c\u5c55\u60c5\u6cc1\uff0c\u653f\u5e9c\u7684\u5de5\u696d\u63aa\u65bd\u8207\u65b9\u6848\uff0c\u4ee5\u53ca\u6211\u570b\u6587\u5316\u80cc\u666f\uff0c\u90fd\u5fc5\u9808\u8207\u5de5\u5ee0\u7ba1\u7406\u7684\u65b9\u6cd5\u7d50\u5408\uff0c\u624d\u80fd\u6210\u70ba\u6700\u4f73\u7684\u7d93\u71df\u8005\u3002", "schedule": "\u7b2c\u4e00\u9031:\u5de5\u5ee0\u7ba1\u7406\u6982\u8ad6\r\n\u7b2c\u4e8c-\u4e94\u9031:\u6a19\u6e96\u5316\u7ba1\u7406\r\n\u7b2c\u516d-\u4e5d\u9031:\u9a57\u8b49\u5236\u5ea6\r\n\u7b2c\u5341-\u5341\u4e09\u9031:\u5275\u65b0\u8207\u4fdd\u8b77\r\n\u7b2c\u5341\u56db-\u5341\u4e03\u9031:\u570b\u969b\u6a19\u6e96\u54c1\u8cea\u7ba1\u7406\u7cfb\u7d71\r\n\u7b2c\u5341\u516b\u9031:\u671f\u672b\u8003", "scorePolicy": "\u671f\u672b\u800360%\uff0c\u7fd2\u984c20%\uff0c\u8207\u4e0a\u8ab2\u8868\u73fe20%", "materials": "\u6559\u5e2b\u81ea\u6709\u8b1b\u7fa9", "foreignLanguageTextbooks": false}]